{
  "name": "demo",
  "version": "0.0.1",
  "private": true,
  "author": "hmpthz",
  "license": "MIT",
  "description": "A boilerplate to develop vbEngine game project using PixiJS v7 library and Vue 3 framework, with VS Code as devtool.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "lint-fix": "eslint . --ext .ts,.vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore --fix",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test:unit": "cypress open --component",
    "test:unit:ci": "cypress run --component --quiet --reporter spec"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pixi/particle-emitter": "~5.0.8",
    "@pixi/sound": "~5.0.0",
    "ajv": "^8.12.0",
    "fontfaceobserver": "^2.3.0",
    "lodash": "^4.17.21",
    "pixi-filters": "~5.2.1",
    "pixi-spine": "~4.0.4",
    "pixi-viewport": "~5.0.0",
    "pixi.js": "~7.2.4",
    "random": "^4.1.0",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "^2.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "16.11",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cypress": "^10.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "start-server-and-test": "^1.14.0",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-static-copy": "^0.13.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.8"
  }
}